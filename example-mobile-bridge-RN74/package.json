{
  "name": "MobileBridgeTemplate_RN74",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "typecheck": "tsc --noEmit",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "build:android": "cd android && ./gradlew assembleDebug && cd ..",
    "build:release": "cd android && ./gradlew assembleRelease && cd ..",
    "clean": "cd android && ./gradlew clean && cd .. && npm run clean:metro",
    "clean:metro": "rm -rf $TMPDIR/metro-* && rm -rf $TMPDIR/haste-*",
    "clean:android": "cd android && ./gradlew clean && cd ..",
    "clean:ios": "cd ios && xcodebuild clean && cd ..",
    "reset": "rm -rf node_modules && rm -rf android/build && rm -rf ios/build && npm install",
    "doctor": "npx react-native doctor",
    "postinstall": "node -e \"try { require('child_process').execSync('pod install', {cwd: 'ios', stdio: 'inherit'}); } catch(e) { console.log('Skipping pod install (iOS only, not available on Windows)'); }\"",
    "postinstall:ios": "cd ios && pod install && cd ..",
    "mode:standalone": "node scripts/switch-mode.js standalone",
    "mode:embedded": "node scripts/switch-mode.js embedded",
    "mode:testhost": "node scripts/switch-mode.js testhost"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^1.23.1",
    "@react-native-community/netinfo": "^11.3.1",
    "react": "18.2.0",
    "react-native": "0.74.5",
    "react-native-safe-area-context": "^4.10.5",
    "react-native-svg": "15.2.0",
    "react-native-url-polyfill": "^2.0.0",
    "react-native-vector-icons": "^10.1.0",
    "react-native-webview": "^13.8.6"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@react-native/babel-preset": "0.74.87",
    "@react-native/eslint-config": "0.74.87",
    "@react-native/metro-config": "0.74.87",
    "@react-native/typescript-config": "0.74.87",
    "@types/jest": "^29.5.2",
    "@types/node": "^20.14.8",
    "@types/react": "^18.2.6",
    "@types/react-native-vector-icons": "^6.4.18",
    "@types/react-test-renderer": "^18.0.0",
    "eslint": "^8.19.0",
    "jest": "^29.6.3",
    "prettier": "2.8.8",
    "react-test-renderer": "18.2.0",
    "typescript": "5.0.4"
  },
  "engines": {
    "node": ">=20"
  }
}
